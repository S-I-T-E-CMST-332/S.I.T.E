extends ../../../layout.pug

block content
    .main.container
        h1 #{client.name} Session Reports

        .row
            .col-md-6.col-md-offset-3
            ul
                each session in sessions
                    li 
                        a(href='/clients/'+ client._id + '/reports/' + session.session_id) #{session.day} 
                else
                    li There are no Sessions